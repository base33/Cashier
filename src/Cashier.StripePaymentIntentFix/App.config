<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
    </startup>
  <appSettings>
    <add key="Cashier:Stripe:Secret" value="{Api key secret}" />
  </appSettings>
  <connectionStrings>
    <remove name="CashierDb" />
    <add name="CashierDb" connectionString="{UMBRACO DB Connection string}" providerName="System.Data.SqlServerCe.4.0" />
  </connectionStrings>
</configuration>
